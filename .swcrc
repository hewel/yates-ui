{
    "jsc": {
        "parser": {
            "syntax": "typescript",
            "tsx": true,
            "decorators": true,
            "dynamicImport": true
        },
        "transform": {
            "react": {
                "runtime": "automatic",
                "importSource": "ags/gtk4",
                "throwIfNamespace": false,
                "useBuiltins": true
            }
        },
        "target": "es2022",
        "loose": false,
        "externalHelpers": false,
        "baseUrl": ".",
        "paths": {
            "gi://*": [
                "/usr/share/ags/types/*"
            ],
            "resource://*": [
                "/usr/share/ags/js/*"
            ]
        }
    },
    "module": {
        "type": "es6",
        "strict": true,
        "strictMode": true
    },
    "sourceMaps": "inline"
}